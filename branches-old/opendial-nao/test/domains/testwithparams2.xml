<?xml version="1.0" encoding="UTF-8"?>

<domain>



	<model type="update" trigger="u_u">


		<rule>
			<case>
				<condition>
					<if var="u_u" value="brilliant" />
				</condition>
				<effect prob="theta_6+theta_7">
					<set var="a_u" value="Approval" />
				</effect>
				<effect prob="theta_8">
					<set var="a_u" value="Irony" />
				</effect>
			</case>
		</rule>


	</model>

</domain>