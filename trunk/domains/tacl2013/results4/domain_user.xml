<?xml version="1.0" encoding="UTF-8"?>

<model trigger="i_u" type="prediction">

	<rule id="usermodel">
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({XX})" />
				<if var="i_u" value="{XX}" />
			</condition>
			<effect prob="0.7065">
				<set var="a_u" value="Confirm" />
			</effect>
			<effect prob="0.2674">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0261"/>
		</case>
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({YY})" />
				<if var="i_u" value="{YY}" relation="!=" />
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.473">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.4428">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0843"/>
		</case>
		<case>
			<condition operator="and">
				<if var="lastMove" value="{i_u}" />
			</condition>
			<effect prob="0.2444">
				<set var="a_u" value="RepeatLast" />
			</effect>
			<effect prob="0.6903">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0653"/>
		</case>
		<case>
			<condition operator="or">
				<if var="a_m" value="AskRepeat" />
				<if var="completed-task" value="true" />
			</condition>
			<effect prob="0.9791">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0209"/>
		</case>
		<case>
			<condition>
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.7584">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.2416"/>
		</case>
	</rule>


	<rule>
		<case>
			<condition>
				<if var="a_m" value="Do(*)" />
			</condition>
			<effect prob="0.1008">
				<set var="a_u" value="Stop" />
			</effect>
			<effect prob="0.0055">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.8937"/>
		</case>
	</rule>

</model>
