<?xml version="1.0" encoding="UTF-8"?>

<model trigger="i_u" type="prediction">

	<rule id="usermodel">
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({XX})" />
				<if var="i_u" value="{XX}" />
			</condition>
			<effect prob="0.6791">
				<set var="a_u" value="Confirm" />
			</effect>
			<effect prob="0.2994">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0215"/>
		</case>
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({YY})" />
				<if var="i_u" value="{YY}" relation="!=" />
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.345">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.3501">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.3049"/>
		</case>
		<case>
			<condition operator="and">
				<if var="lastMove" value="{i_u}" />
			</condition>
			<effect prob="0.1871">
				<set var="a_u" value="RepeatLast" />
			</effect>
			<effect prob="0.6767">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.1361"/>
		</case>
		<case>
			<condition operator="or">
				<if var="a_m" value="AskRepeat" />
				<if var="completed-task" value="true" />
			</condition>
			<effect prob="0.9937">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0063"/>
		</case>
		<case>
			<condition>
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.7499">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.2501"/>
		</case>
	</rule>


	<rule>
		<case>
			<condition>
				<if var="a_m" value="Do(*)" />
			</condition>
			<effect prob="0.1125">
				<set var="a_u" value="Stop" />
			</effect>
			<effect prob="0.021">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.8665"/>
		</case>
	</rule>

</model>
