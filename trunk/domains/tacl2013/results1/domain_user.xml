<?xml version="1.0" encoding="UTF-8"?>

<model trigger="i_u" type="prediction">

	<rule id="usermodel">
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({X})" />
				<if var="i_u" value="{X}" />
			</condition>
			<effect prob="0.6351">
				<set var="a_u" value="Confirm" />
			</effect>
			<effect prob="0.3381">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0268"/>
		</case>
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({Y})" />
				<if var="i_u" value="{Y}" relation="!=" />
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.2452">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.4043">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.3505"/>
		</case>
		<case>
			<condition operator="and">
				<if var="lastMove" value="{i_u}" />
			</condition>
			<effect prob="0.2878">
				<set var="a_u" value="RepeatLast" />
			</effect>
			<effect prob="0.6383">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0739"/>
		</case>
		<case>
			<condition operator="or">
				<if var="a_m" value="AskRepeat" />
				<if var="completed-task" value="true" />
			</condition>
			<effect prob="0.9554">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0446"/>
		</case>
		<case>
			<condition>
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.7711">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.2289"/>
		</case>
	</rule>


	<rule>
		<case>
			<condition>
				<if var="a_m" value="Do(*)" />
			</condition>
			<effect prob="0.0124">
				<set var="a_u" value="Stop" />
			</effect>
			<effect prob="0.0071">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.9805"/>
		</case>
	</rule>

</model>
