<?xml version="1.0" encoding="UTF-8"?>

<model trigger="i_u" type="prediction">

	<rule id="usermodel">
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({XX})" />
				<if var="i_u" value="{XX}" />
			</condition>
			<effect prob="0.6207">
				<set var="a_u" value="Confirm" />
			</effect>
			<effect prob="0.3726">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0066"/>
		</case>
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({YY})" />
				<if var="i_u" value="{YY}" relation="!=" />
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.4538">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.3754">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.1708"/>
		</case>
		<case>
			<condition operator="and">
				<if var="lastMove" value="{i_u}" />
			</condition>
			<effect prob="0.1826">
				<set var="a_u" value="RepeatLast" />
			</effect>
			<effect prob="0.798">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0194"/>
		</case>
		<case>
			<condition operator="or">
				<if var="a_m" value="AskRepeat" />
				<if var="completed-task" value="true" />
			</condition>
			<effect prob="0.9773">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0227"/>
		</case>
		<case>
			<condition>
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.8767">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.1233"/>
		</case>
	</rule>


	<rule>
		<case>
			<condition>
				<if var="a_m" value="Do(*)" />
			</condition>
			<effect prob="0.0286">
				<set var="a_u" value="Stop" />
			</effect>
			<effect prob="0.0806">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.8908"/>
		</case>
	</rule>

</model>
