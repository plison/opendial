<?xml version="1.0" encoding="UTF-8"?>

<model trigger="completed-task,perceived,carried" type="update">

	<rule id="clearorcontinue">
		<case>
			<condition>
				<if var="completed-task" value="true" />
			</condition>
			<effect>
				<clear var="i_u" />
			</effect>
		</case>
		<case>
			<condition>
				<if var="completed-task" value="false" />
			</condition>
			<effect>
				<set var="i_u" value="{i_u}" />
			</effect>
		</case>
	</rule>

	<rule id="moveprior">
		<case>
			<condition>
				<if var="completed-task" value="true" />
			</condition>
			<effect prob="20.8968prior[0]">
				<set var="i_u" value="Move(Left)" />
			</effect>
			<effect prob="20.8968prior[1]">
				<set var="i_u" value="Move(Forward)" />
			</effect>
			<effect prob="20.8968prior[2]">
				<set var="i_u" value="Move(Backward)" />
			</effect>
			<effect prob="20.8968prior[3]">
				<set var="i_u" value="Move(Right)" />
			</effect>
			<effect prob="20.8968prior[4]">
				<set var="i_u" value="Move(Left,Short)" />
			</effect>
			<effect prob="20.8968prior[5]">
				<set var="i_u" value="Move(Forward,Short)" />
			</effect>
			<effect prob="20.8968prior[6]">
				<set var="i_u" value="Move(Backward,Short)" />
			</effect>
			<effect prob="20.8968prior[7]">
				<set var="i_u" value="Move(Right,Short)" />
			</effect>
			<effect prob="20.8968prior[8]">
				<set var="i_u" value="Move(Turn)" />
			</effect>	
			<effect prob="20.8968prior[9]"/>
		</case>
	</rule>
	
	<rule id="perceptionprior">
		<case>
			<condition>
				<if var="perceived" value="[]" relation="!="/>
			</condition>				
			<effect prob="0.2325">
				<set var="i_u" value="DoYouSee(RedObj)" />
			</effect>
			<effect prob="0.0605">
				<set var="i_u" value="DoYouSee(BlueObj)" />
			</effect>
			<effect prob="0.0825">
				<set var="i_u" value="DoYouSee(Something)" />
			</effect>
			<effect prob="0.1583">
				<set var="i_u" value="WhatDoYouSee" />
			</effect>
			<effect prob="0.4662"/>
		</case>
			<case>
			<condition>
				<if var="perceived" value="[]"/>
			</condition>				
			<effect prob="0.0346">
				<set var="i_u" value="DoYouSee(RedObj)" />
			</effect>
			<effect prob="0.0085">
				<set var="i_u" value="DoYouSee(BlueObj)" />
			</effect>
			<effect prob="0.0611">
				<set var="i_u" value="DoYouSee(Something)" />
			</effect>
			<effect prob="0.0612">
				<set var="i_u" value="WhatDoYouSee" />
			</effect>
			<effect prob="0.8347"/>
		</case>
	</rule>
	
	
	<rule id="chitchatprior">
		<case>
			<condition>			
				<if var="lastMove" value="None"/>
			</condition>
			<effect prob="0.2019" >
				<set var="i_u" value="Ask(Name)" />
			</effect>
			<effect prob="0.1347" >
				<set var="i_u" value="Greeting" />
			</effect>
			<effect prob="0.6634"/>
		</case>
		<case>
			<condition>			
				<if var="lastMove" value="Release"/>
			</condition>
			<effect prob="0.2632" >
				<set var="i_u" value="Closing" />
			</effect>
			<effect prob="0.7368"/>
		</case>
		</rule>
	
	
	<rule id="releaseprior">
		<case>
			<condition>
				<if var="completed-task" value="true" />
				<if var="carried" value="[]" relation="!="/>
				<if var="carried" value="None" relation="!="/>
			</condition>
			<effect prob="24.3313prior[0]">
				<set var="i_u" value="Release" />
			</effect>
			<effect prob="24.3313prior[1]" />
		</case>
	</rule>
	

	<rule id="pickupprior">
		<case>
			<condition>
				<if var="completed-task" value="true" />
				<if var="carried" value="[]" />
				<if var="perceived" value="RedObj" relation="contains" />
			</condition>
			<effect prob="0.2389">
				<set var="i_u" value="PickUp(RedObj)" />
			</effect>
			<effect prob="0.0468">
				<set var="i_u" value="PickUp(AtFeet)" />
			</effect>
			<effect prob="0.7143" />
		</case>
			<case>
			<condition>
				<if var="completed-task" value="true" />
				<if var="carried" value="[]" />
				<if var="perceived" value="BlueObj" relation="contains" />
			</condition>
			<effect prob="0.2389">
				<set var="i_u" value="PickUp(BlueObj)" />
			</effect>
			<effect prob="0.0468">
				<set var="i_u" value="PickUp(AtFeet)" />
			</effect>
			<effect prob="0.7143" />
		</case>
		<case>
			<condition>
				<if var="completed-task" value="true" />
				<if var="carried" value="[]" />
			</condition>
			<effect prob="0.1075">
				<set var="i_u" value="PickUp(RedObj)" />
			</effect>
			<effect prob="0.3049">
				<set var="i_u" value="PickUp(BlueObj)" />
			</effect>
			<effect prob="0.144">
				<set var="i_u" value="PickUp(AtFeet)" />
			</effect>
			<effect prob="0.4436" />
		</case>
	</rule>


</model>
