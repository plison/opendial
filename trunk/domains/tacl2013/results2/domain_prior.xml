<?xml version="1.0" encoding="UTF-8"?>

<model trigger="completed-task,perceived,carried" type="update">

	<rule id="clearorcontinue">
		<case>
			<condition>
				<if var="completed-task" value="true" />
			</condition>
			<effect>
				<clear var="i_u" />
			</effect>
		</case>
		<case>
			<condition>
				<if var="completed-task" value="false" />
			</condition>
			<effect>
				<set var="i_u" value="{i_u}" />
			</effect>
		</case>
	</rule>

	<rule id="moveprior">
		<case>
			<condition>
				<if var="completed-task" value="true" />
			</condition>
			<effect prob="26.1724prior[0]">
				<set var="i_u" value="Move(Left)" />
			</effect>
			<effect prob="26.1724prior[1]">
				<set var="i_u" value="Move(Forward)" />
			</effect>
			<effect prob="26.1724prior[2]">
				<set var="i_u" value="Move(Backward)" />
			</effect>
			<effect prob="26.1724prior[3]">
				<set var="i_u" value="Move(Right)" />
			</effect>
			<effect prob="26.1724prior[4]">
				<set var="i_u" value="Move(Left,Short)" />
			</effect>
			<effect prob="26.1724prior[5]">
				<set var="i_u" value="Move(Forward,Short)" />
			</effect>
			<effect prob="26.1724prior[6]">
				<set var="i_u" value="Move(Backward,Short)" />
			</effect>
			<effect prob="26.1724prior[7]">
				<set var="i_u" value="Move(Right,Short)" />
			</effect>
			<effect prob="26.1724prior[8]">
				<set var="i_u" value="Move(Turn)" />
			</effect>	
			<effect prob="26.1724prior[9]"/>
		</case>
	</rule>
	
	<rule id="perceptionprior">
		<case>
			<condition>
				<if var="perceived" value="[]" relation="!="/>
			</condition>				
			<effect prob="0.1132">
				<set var="i_u" value="DoYouSee(RedObj)" />
			</effect>
			<effect prob="0.0085">
				<set var="i_u" value="DoYouSee(BlueObj)" />
			</effect>
			<effect prob="0.0494">
				<set var="i_u" value="DoYouSee(Something)" />
			</effect>
			<effect prob="0.2763">
				<set var="i_u" value="WhatDoYouSee" />
			</effect>
			<effect prob="0.5526"/>
		</case>
			<case>
			<condition>
				<if var="perceived" value="[]"/>
			</condition>				
			<effect prob="0.0813">
				<set var="i_u" value="DoYouSee(RedObj)" />
			</effect>
			<effect prob="0.032">
				<set var="i_u" value="DoYouSee(BlueObj)" />
			</effect>
			<effect prob="0.1027">
				<set var="i_u" value="DoYouSee(Something)" />
			</effect>
			<effect prob="0.0461">
				<set var="i_u" value="WhatDoYouSee" />
			</effect>
			<effect prob="0.7379"/>
		</case>
	</rule>
	
	
	<rule id="chitchatprior">
		<case>
			<condition>			
				<if var="lastMove" value="None"/>
			</condition>
			<effect prob="0.1824" >
				<set var="i_u" value="Ask(Name)" />
			</effect>
			<effect prob="0.1324" >
				<set var="i_u" value="Greeting" />
			</effect>
			<effect prob="0.6852"/>
		</case>
		<case>
			<condition>			
				<if var="lastMove" value="Release"/>
			</condition>
			<effect prob="0.2373" >
				<set var="i_u" value="Closing" />
			</effect>
			<effect prob="0.7627"/>
		</case>
		</rule>
	
	
	<rule id="releaseprior">
		<case>
			<condition>
				<if var="completed-task" value="true" />
				<if var="carried" value="[]" relation="!="/>
				<if var="carried" value="None" relation="!="/>
			</condition>
			<effect prob="13.0611prior[0]">
				<set var="i_u" value="Release" />
			</effect>
			<effect prob="13.0611prior[1]" />
		</case>
	</rule>
	

	<rule id="pickupprior">
		<case>
			<condition>
				<if var="completed-task" value="true" />
				<if var="carried" value="[]" />
				<if var="perceived" value="RedObj" relation="contains" />
			</condition>
			<effect prob="0.0856">
				<set var="i_u" value="PickUp(RedObj)" />
			</effect>
			<effect prob="0.016">
				<set var="i_u" value="PickUp(AtFeet)" />
			</effect>
			<effect prob="0.8984" />
		</case>
			<case>
			<condition>
				<if var="completed-task" value="true" />
				<if var="carried" value="[]" />
				<if var="perceived" value="BlueObj" relation="contains" />
			</condition>
			<effect prob="0.0856">
				<set var="i_u" value="PickUp(BlueObj)" />
			</effect>
			<effect prob="0.016">
				<set var="i_u" value="PickUp(AtFeet)" />
			</effect>
			<effect prob="0.8984" />
		</case>
		<case>
			<condition>
				<if var="completed-task" value="true" />
				<if var="carried" value="[]" />
			</condition>
			<effect prob="0.179">
				<set var="i_u" value="PickUp(RedObj)" />
			</effect>
			<effect prob="0.1286">
				<set var="i_u" value="PickUp(BlueObj)" />
			</effect>
			<effect prob="0.0553">
				<set var="i_u" value="PickUp(AtFeet)" />
			</effect>
			<effect prob="0.6371" />
		</case>
	</rule>


</model>
