<?xml version="1.0" encoding="UTF-8"?>

<model trigger="i_u" type="prediction">

	<rule id="usermodel">
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({XX})" />
				<if var="i_u" value="{XX}" />
			</condition>
			<effect prob="0.6257">
				<set var="a_u" value="Confirm" />
			</effect>
			<effect prob="0.3523">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.022"/>
		</case>
		<case>
			<condition operator="and">
				<if var="a_m" value="Confirm({YY})" />
				<if var="i_u" value="{YY}" relation="!=" />
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.3872">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.399">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.2138"/>
		</case>
		<case>
			<condition operator="and">
				<if var="lastMove" value="{i_u}" />
			</condition>
			<effect prob="0.3558">
				<set var="a_u" value="RepeatLast" />
			</effect>
			<effect prob="0.5184">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.1258"/>
		</case>
		<case>
			<condition operator="or">
				<if var="a_m" value="AskRepeat" />
				<if var="completed-task" value="true" />
			</condition>
			<effect prob="0.9574">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.0426"/>
		</case>
		<case>
			<condition>
				<if var="i_u" value="None" relation="!=" />
			</condition>
			<effect prob="0.8876">
				<set var="a_u" value="{i_u}" />
			</effect>
			<effect prob="0.1124"/>
		</case>
	</rule>


	<rule>
		<case>
			<condition>
				<if var="a_m" value="Do(*)" />
			</condition>
			<effect prob="0.0776">
				<set var="a_u" value="Stop" />
			</effect>
			<effect prob="0.0797">
				<set var="a_u" value="Disconfirm" />
			</effect>
			<effect prob="0.8427"/>
		</case>
	</rule>

</model>
