<model type="update" trigger="completed-task,perceived,carried">

<rule id="clearorcontinue">
<case>
<condition>
<if var="completed-task" value="true" />
</condition>
<effect>
<clear var="i_u"/>
</effect>
</case>
<case>
<condition>
<if var="completed-task" value="false" />
</condition>
<effect>
<set var="i_u" value="{i_u}" /></effect>
</case>
</rule>

