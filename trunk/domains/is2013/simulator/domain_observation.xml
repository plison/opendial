<model trigger="a_u0" type="prediction" id="observationNoise">

	<rule id="obs_sim">
		<case>
			<condition>
				<if var="a_u0" value="None" relation="!=" />
			</condition>
			<effect prob="asrScore[0]">
				<set var="a_u" var2="a_u0" />
			</effect>
			<effect prob="asrScore[1]">
				<set var="a_u" var2="a_uother" />
			</effect>
			<effect prob="asrScore[2]">
