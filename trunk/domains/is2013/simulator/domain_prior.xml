<?xml version="1.0" encoding="UTF-8"?>

<model trigger="floor" type="update">

	<rule id="clear_start_sim">
		<case>
			<condition>
				<if var="floor" value="start" />
			</condition>
			<effect>
				<clear var="i_u" />
			</effect>
		</case>
	</rule>

	<rule id="continue_sim">
		<case>
			<condition>
				<if var="floor" value="continue" />
			</condition>
			<effect>
				<set var="i_u" value="{i_u}" />
			</effect>
		</case>
	</rule>

	<rule>
		<case>
			<condition>
				<if var="floor" value="start" />
			</condition>
			<effect prob="0.6785">
				<set var="i_u" value="Move(Left)" />
			</effect>
			<effect prob="0.0457">
				<set var="i_u" value="Move(Forward)" />
			</effect>
			<effect prob="0.05">
				<set var="i_u" value="Move(Backward)" />
			</effect>
			<effect prob="0.014">
				<set var="i_u" value="Move(Right)" />
			</effect>
			<effect prob="0.095">
				<set var="i_u" value="DoYouSee(o1)" />
			</effect>
			<effect prob="0.02">
				<set var="i_u" value="DoYouSee(o2)" />
			</effect>
			<effect prob="0.0266">
				<set var="i_u" value="WhatDoYouSee" />
			</effect>
		</case>
	</rule>

	<rule>
		<quantifier id="X" />
		<case>
			<condition>
				<if var="floor" value="start" />
				<if var="carried" value="{X}" relation="contains" />
			</condition>
			<effect prob="0.09">
				<set var="i_u" value="Release({X})" />
			</effect>
			<effect prob="0.91"/>
		</case>
	</rule>

	<rule>
		<case>
			<condition>
				<if var="floor" value="start" />
				<if var="carried" value="[]" />
			</condition>
			<effect prob="0.085">
				<set var="i_u" value="PickUp(o1)" />
			</effect>
			<effect prob="0.085">
				<set var="i_u" value="PickUp(o2)" />
			</effect>
			<effect prob="0.83"/>
		</case>
	</rule>



</model>