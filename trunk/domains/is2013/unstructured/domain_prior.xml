<?xml version="1.0" encoding="UTF-8"?>

<model trigger="floor" type="update">

	<rule id="clearorcontinue">
		<case>
			<condition>
				<if var="floor" value="start" />
			</condition>
			<effect>
				<clear var="i_u" />
			</effect>
		</case>
		<case>
			<condition>
				<if var="floor" value="continue" />
			</condition>
			<effect>
				<set var="i_u" value="{i_u}" />
			</effect>
		</case>
	</rule>

	<rule id="baseprior">
		<case>
			<condition>
				<if var="floor" value="start" />
			</condition>
			<effect prob="0.125">
				<set var="i_u" value="Move(Left)" />
			</effect>
			<effect prob="0.125">
				<set var="i_u" value="Move(Forward)" />
			</effect>
			<effect prob="0.125">
				<set var="i_u" value="Move(Backward)" />
			</effect>
			<effect prob="0.125">
				<set var="i_u" value="Move(Right)" />
			</effect>
			<effect prob="0.125">
				<set var="i_u" value="DoYouSee(o1)" />
			</effect>
			<effect prob="0.125">
				<set var="i_u" value="DoYouSee(o2)" />
			</effect>
			<effect prob="0.125">
				<set var="i_u" value="WhatDoYouSee" />
			</effect>
			<effect prob="0.125" />
		</case>
	</rule>
	
	
	<rule id="releaseprior">
		<case>
			<condition>
				<if var="floor" value="start" />
				<if var="carried" value="o1" relation="contains" />
			</condition>
			<effect prob="0.15">
				<set var="i_u" value="Release(o1)" />
			</effect>
			<effect prob="0.85" />
		</case>
			<case>
			<condition>
				<if var="floor" value="start" />
				<if var="carried" value="o2" relation="contains" />
			</condition>
			<effect prob="0.15">
				<set var="i_u" value="Release(o2)" />
			</effect>
			<effect prob="0.85" />
		</case>
		<case>
			<condition>
				<if var="floor" value="start" />
			</condition>
			<effect prob="0.0">
				<set var="i_u" value="Release(o1)" />
			</effect>
			<effect prob="1.0" />
		</case>
	</rule>

	<rule id="pickupprior">
		<case>
			<condition>
				<if var="floor" value="start" />
				<if var="carried" value="[]" />
			</condition>
			<effect prob="0.15">
				<set var="i_u" value="PickUp(o1)" />
			</effect>
			<effect prob="0.15">
				<set var="i_u" value="PickUp(o2)" />
			</effect>
			<effect prob="0.7" />
		</case>
		<case>
			<condition>
				<if var="floor" value="start" />
			</condition>
			<effect prob="0.0">
				<set var="i_u" value="PickUp(o1)" />
			</effect>
			<effect prob="0.0">
				<set var="i_u" value="PickUp(o2)" />
			</effect>
			<effect prob="1.0" />
		</case>
	</rule>



</model>