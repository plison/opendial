
<model type="systemActionValue">

<rule>
	<input>
		<var label="floor"/>
	</input>
	<output>
		<var label="a_m"/>
	</output>
	<case>
		<condition><if var="floor" value="init"/></condition>
		<effect operator="and">
			<set var="a_m" value="RequestIntent"/>
		</effect>
	</case></rule>

<rule>
	<input>
		<var label="floor"/>
		<var type="intent" denotation="i"/>
		<var feature="obj" base="i" denotation="o"/>
	</input>
	<output>
		<var label="a_m"/>
	</output>
	<case>
		<condition>
			<if var="floor" value="free"/>
			<if var="i" value="Want"/>
			<if var="o" value="X"/>
		</condition>
		<effect prob="1.0"><set var="a_m" value="DoX"/></effect>
	</case>
	<case>
		<condition>
			<if var="floor" value="free"/>
			<if var="i" value="Want"/>
			<if var="o" value="Y"/>
		</condition>
		<effect prob="0.0"><set var="a_m" value="DoX"/></effect>
	</case>
</rule>

<rule>
	<input>
		<var label="floor"/>
		<var type="intent" denotation="i"/>
		<var feature="obj" base="i" denotation="o"/>
	</input>
	<output>
		<var label="a_m"/>
	</output>
	<case>
		<condition>
			<if var="floor" value="free"/>
			<if var="i" value="Want"/>
			<if var="o" value="X"/>
		</condition>
		<effect prob="1.0"><set var="a_m" value="DoY"/></effect>
	</case>
	<case>
		<condition>
			<if var="floor" value="free"/>
			<if var="i" value="Want"/>
			<if var="o" value="Y"/>
		</condition>
		<effect prob="0.0"><set var="a_m" value="DoY"/></effect>
	</case>
</rule>

<rule>
	<input>
		<var label="floor"/>
		<var type="intent" denotation="i"/>
	</input>
	<output>
		<var label="a_m"/>
	</output>
	<case>
		<condition>
			<if var="floor" value="free"/>
		</condition>
		<effect prob="0.6"><set var="a_m" value="AskRepeat"/></effect>
	</case>
</rule>


<rule>
	<input>
		<var label="floor"/>
		<var label="a_u"/>
	</input>
	<output>
		<var label="a_m"/>
	</output>
	<case>
		<condition>
			<if var="floor" value="free"/>
			<if var="a_u" value="Unknown"/>
		</condition>
		<effect prob="1.0"><set var="a_m" value="InformNotUnderstand"/></effect>
	</case>
</rule>

</model>