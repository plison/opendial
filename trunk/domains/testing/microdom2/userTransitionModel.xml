
<model type="userTransition">

<rule>
	<input>
		<var label="a_u"/>
		<var feature="obj" base="a_u" id="o1"/>
		<var count="intent" id="intent_count"/>
	</input>
	<output>
			<var type="intent" id="i"/>
			<var feature="obj" base="i" id="o2"/>
	</output>
	<case>
		<condition operator="and">
			<if var="a_u" value="AskFor"/>
			<if var="o1" value="X"/>
			<if var="intent_count" value="0"/>
		</condition>
		<effect prob="1.0">
			<add var="i"/>
			<set var="i" value="Want"/>
			<set var="o2" value="X"/>
		</effect>
	</case>
</rule>


<rule>
	<input>
		<var label="a_u"/>
		<var feature="obj" base="a_u" id="o1"/>
		<var count="intent" id="intent_count"/>
	</input>
	<output>
		<var type="intent" id="i"/>
		<var feature="obj" base="i" id="o2"/>
	</output>
	<case>
		<condition operator="and">
			<if var="a_u" value="AskFor"/>
			<if var="o1" value="Y"/>
			<if var="intent_count" value="0"/>
		</condition>
		<effect prob="1.0">
			<add var="i"/>
			<set var="i" value="Want"/>
			<set var="o2" value="Y"/>
		</effect>
	</case>
</rule>

<rule>
	<input>
		<var label="floor"/>
	</input>
	<output>
		<var label="floor"/>
	</output>
	<case>
		<condition operator="or"><if var="floor" value="wait"/><if var="floor" value="free"/><if var="floor" value="init"/></condition>
		<effect><set var="floor" value="free"/></effect>
	</case></rule>

</model>