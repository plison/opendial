
<model type="userPrediction">

<rule>
	<output>
		<var label="a_u"/>
	</output>
	<case>
		<effect prob="0.33"><set var="a_u" value="AskForX"/></effect>
		<effect prob="0.33"><set var="a_u" value="AskForY"/></effect>
		<effect prob="0.34"><set var="a_u" value="Unknown"/></effect>
	</case>
</rule>


<rule>
	<input>
		<instance denotation="i" category="intent"/>
		<var label="a_m"/>
	</input>
	<output>
		<var label="a_u"/>
	</output>
	<case>
		<condition>
			<if var="i" value="WantX"/>
			<if var="a_m" value="AskRepeat"/>
		</condition>
		<effect prob="1.0"><set var="a_u" value="AskForX"/></effect>
	</case>
	<case>
		<condition>
			<if var="i" value="WantY"/>
			<if var="a_m" value="AskRepeat"/>
		</condition>
		<effect prob="1.0"><set var="a_u" value="AskForY"/></effect>
	</case>
</rule>

</model>