
<model type="userPrediction">

<rule>
	<output>
		<var label="a_u"/>
		<var feature="obj" base="a_u" denotation="o"/>
	</output>
	<case>
		<effect prob="0.33">
			<set var="a_u" value="AskFor"/>
			<set var="o" value="X"/>
		</effect>	
		<effect prob="0.33">
			<set var="a_u" value="AskFor"/>
			<set var="o" value="Y"/>
		</effect>
		<effect prob="0.34"><set var="a_u" value="Unknown"/></effect>
	</case>
</rule>


<rule>
	<input>
		<var denotation="i" type="intent"/>
		<var feature="obj" base="i" denotation="o1"/>
		<var label="a_m"/>
	</input>
	<output>
		<var label="a_u"/>
		<var feature="obj" base="a_u" denotation="o2"/>
	</output>
	<case>
		<condition>
			<if var="i" value="Want"/>
			<if var="o1" value="X"/>
			<if var="a_m" value="AskRepeat"/>
		</condition>
		<effect prob="1.0">
			<set var="a_u" value="AskFor"/>
			<set var="o2" value="X"/>
		</effect>
	</case>
	<case>
		<condition>
			<if var="i" value="Want"/>
			<if var="o1" value="Y"/>
			<if var="a_m" value="AskRepeat"/>
		</condition>
		<effect prob="1.0">
			<set var="a_u" value="AskFor"/>
			<set var="o2" value="Y"/>
		</effect>
	</case>
</rule>

</model>