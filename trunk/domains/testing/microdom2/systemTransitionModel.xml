
<model type="systemTransition">

<rule>
	<input>
		<var label="a_m"/>
		<var id="i" type="intent"/>
	</input>
	<output>
		<var pointer="i"/>
	</output>
	<case>
		<condition><if var="a_m" value="DoX"/></condition>
		<effect prob="1.0"><remove var="i"/></effect>
	</case>
	<case>
		<condition><if var="a_m" value="DoY"/></condition>
		<effect prob="1.0"><remove var="i"/></effect>
	</case>
</rule>

<rule>
	<input>
		<var label="floor"/>
		<var label="a_m"/>
	</input>
	<output>
		<var label="floor"/>
	</output>
	<case>
		<condition operator="or"><if var="floor" value="free"/><if var="floor" value="init"/></condition>
		<effect><set var="floor" value="wait"/></effect>
	</case>
</rule>

</model>