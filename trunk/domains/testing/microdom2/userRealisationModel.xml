
<model type="userRealisation">

<rule>
	<input>
		<var label="a_u"/>
		<var feature="obj" base="a_u" id="o"/>
	</input>
	<output><var label="doXObs"/></output>
	<case>
		<condition>
			<if var="a_u" value="AskFor"/>
			<if var="o" value="X"/>
		</condition>
		<effect prob="1.0"><set var="doXObs" value="true"/></effect>
	</case>
</rule>


<rule>
	<input>
		<var label="a_u"/>
		<var feature="obj" base="a_u" id="o"/>
	</input>
	<output><var label="doYObs"/></output>
	<case>
		<condition>
			<if var="a_u" value="AskFor"/>
			<if var="o" value="Y"/>
		</condition>
		<effect prob="1.0"><set var="doYObs" value="true"/></effect>
	</case>
</rule>


<rule>
	<input><var label="a_u"/></input>
	<output><var label="yesObs"/></output>
	<case>
		<condition><if var="a_u" value="Confirm"/></condition>
		<effect prob="1.0"><set var="yesObs" value="true"/></effect>
	</case>
</rule>


<rule>
	<input><var label="a_u"/></input>
	<output><var label="noObs"/></output>
	<case>
		<condition><if var="a_u" value="Deny"/></condition>
		<effect prob="1.0"><set var="noObs" value="true"/></effect>
	</case>
</rule>

<rule>
	<input><var label="a_u"/></input>
	<output><var label="anythingObs"/></output>
	<case>
		<condition><if var="a_u" value="Unknown"/></condition>
		<effect prob="1.0"><set var="anythingObs" value="true"/></effect>
	</case>
</rule>

</model>
