     <item>one</item>
	<item>zero</item>
	<item>two</item>
	<item>three</item>
	<item>four</item>
	<item>five</item>
	<item>six</item>
	<item>seven</item>
	<item>eight</item>
	<item>nine</item>
	<item>ten</item>
	<item>eleven</item>
	<item>twelve</item>
     </one-of>
</item>
   </rule>
</grammar>
